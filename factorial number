// find the factorial of a number
import java.util.Scanner;
class Main {
    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        System.out.print("Enter N:");
        int n = in.nextInt();
        int f =1;
        for(int i=1; i<=n; i++){
            f = f*i;
        }
        System.out.print(f);
    }
}
