//Smallest numbrt in an array
import java.util.Scanner;
import java.util.*;

class Main {
    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        int[] arr = new int[5];
        for(int i =0; i<5; i++){
            arr[i]=in.nextInt();
        }
        
        int l=arr[0];
        for(int i=0; i<arr.length; i++){
            if(arr[i]<l){
                l = arr[i];
            }
        }
        System.out.print("Smallest no:"+l);
    }
    
}
